{
    "name": "dgtlss/columbus",
    "description": "Plugin to the Cooker Engine! Easy site building by Genericmilk",
    "keywords": ["laravel", "sitemap"],
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Langer",
            "email": "nathanlanger@googlemail.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "nunomaduro/laravel-desktop-notifier": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Dgtlss\\Columbus\\": "src/"
        },
        "files": [
			"src/helpers.php"
		]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        },
        "laravel": {
            "providers": [
                "Dgtlss\\Columbus\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/framework": "^9.0.0"
    }
}
